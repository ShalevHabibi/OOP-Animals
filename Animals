public class Animals {
        protected String name;
        protected int age =0;

        //constructor
        public Animals(String name, int age) {
            this.name = name;
            this.age = age;
        }
        //methods
        public void eat() {
            System.out.println(name + " is eating");
        }
        public void sleep() {
            System.out.println(name + " is sleeping");
        }

    }

